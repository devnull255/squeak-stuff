TestCase subclass: #DatageneratorTest	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Productivity-Tools-Tests'!!DatageneratorTest methodsFor: 'testing' stamp: 'mdm 5/13/2014 19:14'!testAccessors	"answers sender by testing Datagenerator accessors"	| datagenerator lastNames firstNames cities states  |      datagenerator := Datagenerator new.	states :=  datagenerator states.      self shouldnt: [states isNil].      self should: [states size > 0].             firstNames := datagenerator firstNames.       self shouldnt: [firstNames isNil].       self should: [firstNames size > 0].       lastNames := datagenerator lastNames.       self shouldnt: [lastNames isNil].       self should: [lastNames size > 0].              cities := datagenerator cities.       self shouldnt: [cities isNil].       self should: [cities size > 0].       ! !!DatageneratorTest methodsFor: 'testing' stamp: 'mdm 5/14/2014 12:44'!testGenerators	"answers sender with a test of the generated string functions"	| dg numString alphaString alphaNumString upperAlphaString mixedAlpha phoneNum ssnNum |	dg := Datagenerator new.      alphaString := dg alpha: 10.      self should: [alphaString size = 10].            numString := dg numeric: 15.      self should: [numString size = 15].      alphaNumString := dg alphaNumeric: 8.      self should: [alphaNumString size = 8].      upperAlphaString := dg upperAlpha: 20.      self should: [upperAlphaString size = 20].      mixedAlpha := dg mixedAlpha: 50.      self should: [mixedAlpha size = 50].      phoneNum := dg phoneNumber.      self should: [phoneNum size = 12].      ssnNum := dg ssn.      self should: [ssnNum size = 11].! !